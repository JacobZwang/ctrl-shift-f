{
	"manifest_version": 3,
	"name": "Cache Search",
	"version": "0.1.0",
	"description": "Search cached pages for text.",
	"icons": {},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"exclude_matches": [
				"http://localhost:8080/*",
				"https://jacobzwang.github.io/ctrl-shift-f/*"
			],
			"js": ["./index.js"],
			"run_at": "document_end"
		}
	],
	"permissions": ["storage"],
	"action": {
		"default_popup": "popup.html"
	}
}
